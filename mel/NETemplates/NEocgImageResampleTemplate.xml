
<?xml version='1.0' encoding='UTF-8'?>
<templates>
    <using package='maya'/>
    <template name='NEocgImageResample'>
        <attribute name='inStream' type='maya.TdataCompound'>
            <label>In Stream</label>
        </attribute>
        <attribute name='outStream' type='maya.TdataCompound'>
            <label>Out Stream</label>
        </attribute>
    </template>
    <view name='NEDefault' template='NEocgImageResample'>
        <property name='outStream'/>
        <property name='inStream'/>
        <property name='enable'/>
    </view>
    <view name='NEDefaultSoloOutput' template='NEocgImageResample'>
        <property name='outStream'/>
    </view>
</templates>
